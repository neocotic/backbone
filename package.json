{
  "name": "hipbone",
  "description": "Fork of Backbone.js which supports and is written using ES2015 classes",
  "version": "1.2.3",
  "keywords": [
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser",
    "backbone"
  ],
  "bugs": {
    "url": "https://github.com/neocotic/hipbone/issues"
  },
  "license": "MIT",
  "author": "Alasdair Mercer",
  "main": "hipbone.js",
  "dependencies": {
    "underscore": ">=1.7.0"
  },
  "devDependencies": {
    "docco": "^0.7.0",
    "jquery": "^2.1.4",
    "node-qunit-phantomjs": "^1.3.0",
    "qunitjs": "^1.20.0",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "test": "node-qunit-phantomjs test/index.html",
    "build": "uglifyjs hipbone.js --mangle --source-map hipbone.min.map -o hipbone.min.js",
    "doc": "docco hipbone.js",
    "lint": "jsl -nofilelisting -nologo -conf docs/jsl.conf -process hipbone.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neocotic/hipbone.git"
  },
  "files": [
    "hipbone.js",
    "hipbone.min.js",
    "hipbone.min.map"
  ],
  "engines": {
    "node": ">=4.0"
  }
}
